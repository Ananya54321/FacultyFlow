<% layout("/layouts/boilerplate")-%>

<body>
  <p>All Departments:</p>
  <% for(let department of departmentList){ %>
  <p><%= department.deptname %></p>
  <form action="/departments/<%= department._id %>" method="get">
    <button>show details</button>
  </form>
  <form action="/departments/<%= department._id %>/announcements" method="get">
    <button>show announcements</button>
  </form>
  <% } %>
</body>
